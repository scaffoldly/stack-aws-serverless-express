{
  "name": "stack",
  "description": "Serverless Stack",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "npx -s --yes @devcontainers/cli up --workspace-folder .",
    "prepare": "husky && yarn dotenv && yarn tsoa",
    "start": "serverless offline",
    "build": "serverless package",
    "deploy": "serverless deploy",
    "tsoa": "serverless tsoa",
    "serverless": "serverless",
    "dotenv": "dotenv-out -f typescript -e .env -e .scaffoldly/.env -c $NODE_ENV -o src",
    "lint": "eslint '*/**/*.{js,cjs,ts,tsx}' --quiet --fix",
    "format": "prettier --write '*/**/*.{js,cjs,ts,tsx}'"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.77",
    "@types/cookies": "0.7.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.11",
    "@types/mime-types": "^2.1.4",
    "@types/node": ">=18",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "dotenv-out": "^1.0.6",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.1.1",
    "pretty-quick": "^4.0.0",
    "serverless": "3",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.50.1",
    "serverless-offline": "^13.3.3",
    "serverless-offline-resources": "^0.1.19",
    "serverless-react": "^0.2.7",
    "serverless-tsoa": "^0.1.3",
    "ts-node": "^10.9.1",
    "typescript": "5.x"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.x",
    "@aws-sdk/client-secrets-manager": "3.x",
    "@aws-sdk/client-sns": "3.x",
    "@aws-sdk/client-sqs": "3.x",
    "@aws-sdk/lib-dynamodb": "3.x",
    "@aws-sdk/util-dynamodb": "3.x",
    "@tsoa/cli": "^6.0.0",
    "@vendia/serverless-express": "^4.5.2",
    "@vitejs/plugin-react": "^4.2.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.3",
    "bs58": "^5.0.0",
    "cookies": "0.8.0",
    "cors": "^2.8.5",
    "ddb-table": "^2.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "jose": "2.0.5",
    "mime-types": "^2.1.35",
    "morgan-body": "2.x",
    "path-parser": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsoa": "^6.0.1",
    "uuid": "9.x",
    "vite": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,cjs,ts,tsx}": [
      "pretty-quick --staged",
      "eslint --fix",
      "git add"
    ]
  }
}
